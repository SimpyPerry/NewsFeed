@model IEnumerable<NewsFeedDTO>

@{
    ViewData["Title"] = "Home Page";
}


<div>
    @foreach(var news in Model)
    {
        <div>
            <h3>@news.Title</h3>
            <ul>
                <li>@news.Author</li>
                <li>@news.Content</li>
                <li>@news.CreatedDate</li>
                <li>@news.UpdatedDate</li>
                <li>@news.HashTags</li>
               
                
                
            </ul>
            @Html.ActionLink("Slet", "Delete"
           , "News", new { id = news.NewsId })
            
        </div>
    }
</div>